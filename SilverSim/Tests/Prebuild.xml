<?xml version="1.0"?>
<Project frameworkVersion="v4_5" name="SilverSim.Tests" path="SilverSim/Tests" type="Library">
  <Configuration name="Release">
    <Options>
      <OutputPath>../../bin/plugins/</OutputPath>
    </Options>
  </Configuration>
  <Configuration name="Debug">
    <Options>
      <OutputPath>../../bin/plugins/</OutputPath>
    </Options>
  </Configuration>
  <ReferencePath>../../bin/</ReferencePath>
  <Reference name="log4net" path="../../bin/" localCopy="false"/>
  <Reference name="System"/>
  <Reference name="System.Core"/>
  <Reference name="System.Xml"/>
  <Reference name="System.Web"/>
  <Reference name="System.Data"/>
  <Reference name="SilverSim.Types" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.ServiceInterfaces" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Scene.Types" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Scene.Management" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Scene.ServiceInterfaces" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Main.Common" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Scripting.Common" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Viewer.Messages" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Viewer.Core" path="../../bin/" localCopy="false"/>
  <Reference name="MySql.Data" path="../../bin/" localCopy="false"/>
  <Reference name="SilverSim.Threading" localCopy="false"/>
  <Reference name="Nini" path="../../bin/" localCopy="false"/>
  <Files>
    <Match pattern="*.cs" recurse="true">
        <Exclude pattern="TemporaryGeneratedFile"/>
    </Match>
    <Match buildAction="EmbeddedResource" path="Resources" pattern="*" recurse="true"/>
    <Match pattern="ProductAssemblyInfo.cs" path=".." recurse="false"/>
    <Match pattern="Prebuild.xml" recurse="false" buildAction="None"/>
    <Match pattern="Product.licenseheader" path=".." recurse="false" buildAction="None"/>
  </Files>
</Project>
